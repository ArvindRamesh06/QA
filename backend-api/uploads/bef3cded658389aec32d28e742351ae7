    transactionRelatedPartyAccount:
      description: Information on the account payer (deptor) or payee (creditor). It is according to the payment direction, and filled in in the case of counterparty.
      type: object
      required:
        - identification
      properties:
        identification:
          description: Identification of account payer (deptor) or payee (creditor) type.
          type: object
          properties:
            iban:
              description: Payer (deptor) or payee (creditor) account no. in the international format of IBAN account no.
              type: string
              example: 'CZ0827000000002108589434'
            other:
              description: Payer (deptor) or payee (creditor) account no. in other/local format of account no. Not implemented.
              type: object
              required:
                - identification
              properties:
                identification:
                  description: The value of payer (deptor) or payee (creditor) account no. in other/local format of account no.
                  type: string
                  example: '0000002108589434'
        currency:
          description: Payer (deptor) or payee (creditor) account currency.
          allOf:
            - $ref: '#/components/schemas/currencyCode'
        name:
          description: Payer (deptor) or payee (creditor) account name.
          type: string
          example: 'Jan Novák'
    transactionRelatedAgentObject:
      description: Information on the payer (deptor) or payee (creditor) bank. It is according to the payment direction, and filled in in the case of counterparty.
      type: object
      required:
        - financialInstitutionIdentification
      properties:
        financialInstitutionIdentification:
          description: Payer (deptor) or payee (creditor) bank code in the international format BIC / SWIFT code.
          type: object
          properties:
            bic:
              description: BIC / SWIFT code of the payer (deptor) or payee (creditor) bank
              type: string
              example: 'BACXCZPP'
            name:
              description: Payer (deptor) or payee (creditor) bank code as stated in the code list of world banks (SWIFT Directory).
              type: string
              example: 'UNCRITMMXXX'
            other:
              description: Other payer (deptor) or payee (creditor) bank identification.
              type: object
              required:
                - identification
              properties:
                identification:
                  description: Other payer (deptor) or payee (creditor) bank identification, mainly in the form of the so-called local bank code.
                  type: string
                  example: '2700'
    remittanceInformation:
      description: Additional payment information.
      type: object
      properties:
        unstructured:
          description: Additional payment information filled in in the payment as unstructured information.  If the payment states several repetitions of unstructured record supplementing payment information, then only the first of them will be considered.
          type: string
          example: '/VS/123456/KS/456789/SS/879213546'
        structured:
          description: The completion of payment symbols (variable, specific and constant) is expected.If the payment does not have a variable, specific or constant symbol, then the entire Structured Remittance Information will be left empty.
          type: object
          properties:
            creditorReferenceInformation:
              description: To display information on variable, specific and constant symbol.
              type: object
              properties:
                reference:
                  description: "Start the field with the VS code: (for variable symbol), or SS: (for specific symbol) or KS: (for constant symbol). In one repetition of the reference field, all three symbols may be stated, each time maximum one repetition of each individual symbol.The method of recording symbols is VS:|KS:|SS:)[0- 9]{1,10}."
                  type: string
                  example: '"VS:123456","KS:456789","SS:879213546"'
    usersAccountTransaction:
      description: Transaction record
      type: object
      required:
        - amount
        - creditDebitIndicator
        - status
        - bookingDate
        - valueDate
        - bankTransactionCode
        - entryDetails
      properties:
        entryReference:
          description: Identification no. of the payment assigned by the bank.
          type: string
          example: 'RB-4567813'
        amount:
          allOf:
            - $ref: "#/components/schemas/amount"
          properties:
            value:
              example: 105.25
        creditDebitIndicator:
          $ref: "#/components/schemas/creditDebitIndicator"
        status:
          description: Transaction status,  realized transactions with the constant BOOK, or yeat unrealized transactions with the constant PDNG.
          type: string
          enum: ['BOOK', 'PDNG']
          example: 'BOOK'
        bookingDate:
          description: Date of processing of the payment by the bank
          allOf:
            - $ref: '#/components/schemas/usersAccountTransactionDateObject'
        valueDate:
          description: Valuta date
          allOf:
            - $ref: '#/components/schemas/usersAccountTransactionDateObject'
        bankTransactionCode:
          $ref: '#/components/schemas/bankTransactionCode'
        entryDetails:
          description: Transaction details. The level repeats only for the given item
          type: object
          required:
            - transactionDetails
          properties:
            transactionDetails:
              description: Payment details. The level repeats only for the given item.
              type: object
              required:
                - relatedParties
                - relatedAgents
              properties:
                references:
                  description: A set of references unambiguously identifying the payment.
                  type: object
                  properties:
                    paymentId:
                      description: Id of initiated payment. The identifier of the payment from the PISP service
                      type: string
                      example: '898165198413'
                    paymentInformationIdentification:
                      description: Specific symbol
                      type: string
                      example: 'xxxxxxxxxxxx1248'
                    instructionIdentification:
                      description: Constant symbol
                      type: string
                      example: '54321'
                    endToEndIdentification:
                      description: Variable symbol
                      type: string
                      example: '0250117002'
                    chequeNumber:
                      description: For cheque transactions there may be a cheque number; for card transactions there may be a card number with asterisks. The card no. is each time in the format xxxxxxxxxxxx1234 where according to the standard, only 0-9 may be used (8-28 char.) i.e. without illustrative xxxx.
                      type: string
                      example: 'xxxxxxxxxxxx1248'
                amountDetails:
                  description: Details of the payment amount, especially if it is a conversion payment or cashback.
                  type: object
                  properties:
                    instructedAmount:
                      description: Amount and currency of payment in the currency that was required by the client for transfer. E.g., for intra-bank payments, the payer account currency and the payment currency if the client required the payment in the payer account currency.
                      type: object
                      properties:
                        amount:
                          allOf:
                            - $ref: '#/components/schemas/amount'
                          properties:
                            value:
                              example: 10
                            currency:
                              example: 'GBP'
                    counterValueAmount:
                      description: The amount and currency of payment in the client account currency after conversion of amount that was required by the client for transfer.
                      type: object
                      required:
                        - amount
                      properties:
                        amount:
                          allOf:
                            - $ref: '#/components/schemas/amount'
                          properties:
                            value:
                              example: 105.25
                        currencyExchange:
                          description: Information on currencies and exchange rates used.
                          type: object
                          required:
                            - sourceCurrency
                            - exchangeRate
                          properties:
                            sourceCurrency:
                              description: Client account currency (source/original currency/payer account currency for intrabank conversion payments).
                              allOf:
                                - $ref: '#/components/schemas/currencyCode'
                            targetCurrency:
                              description: Payment currency (End/target currency/payee account currency for intrabank conversion payments).
                              example: 'GBP'
                              allOf:
                                - $ref: '#/components/schemas/currencyCode'
                            exchangeRate:
                              description: The exchange rate used to charge a payment. Only one rate is filled in, even in the case of cross conversion.
                              type: number
                              example: 10.525
                relatedParties:
                  description: Information on the payer, the payer's account and the original payer and payee, the payee's account and the final payee in the payment.
                  type: object
                  required:
                    - debtorAccount
                    - creditorAccount
                  properties:
                    debtor:
                      description: Information on the payer. It is according to the payment direction, and filled in in the case of counterparty.

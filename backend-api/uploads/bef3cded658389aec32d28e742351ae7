    paymentIdentification:
      description: Payment identification
      type: object
      properties:
        instructionIdentification:
          description: Instruction identification. TPP can provide an optional identification to pair/validate instruction on their side.
          type: string
          example: 'NejakeID41785962314574'
    paymentIdentificationInResponse:
      description: Payment identification
      type: object
      required:
        - transactionIdentification
      properties:
        instructionIdentification:
          description: Instruction identification (if provided in request)
          type: string
          example: 'NejakeID41785962314574'
        transactionIdentification:
          description: Transaction identification
          type: string
          example: '033225570000078020091'
    paymentTypeInformation:
      description: Payment type information
      type: object
      properties:
        instructionPriority:
          description: >
            Instruction priority

            If the element is not filled in, the payer's provider assigns a NORM value and makes a payment by default.

            "NORM" means same as in our mobile app -- use instant payment if available, otherwise standard payment

            "INST" means that we want instant payment, and if it is not available at the time of this api call, you would get an error.

            In case of availability of the instant payment changes between the payment initiation and the payment processing, there is fall back to a standard payment.

          type: string
          enum: ['NORM', 'INST']
          example: 'NORM'
    paymentTypeInformationInResponse:
      allOf:
        - $ref: '#/components/schemas/paymentTypeInformation'
      required:
        - serviceLevel
      properties:
        serviceLevel:
          description: Service level
          type: object
          required:
            - code
          properties:
            code:
              description: >
                Service level Code


                DMCT


                [DoMestic Credit Transfer] Domestic payment


                ESCT


                [SEPA Credit Transfer] – SEPA paymen


                XBCT


                [Cross-Border Credit Transfer] – Cross border payment[SEPA Credit Transfer] – SEPA payment


                EXCT


                [EEA X-border Credit Transfers] – Cross border payment within the EEP


                NXCT


                [Non-EEA X-border Credit Transfers] – Cross border payment outside the EEP

              type: string
              enum: ['DMCT', 'ESCT', 'XBCT', 'EXCT', 'NXCT']
              example: 'DMCT'
    signInfo:
      description: Status information and id of unauthorized transactions
      type: object
      required:
        - state
      properties:
        state:
          allOf:
            - $ref: '#/components/schemas/authorizationState'
        signId:
          description: Identifier of the authorization process of a particular transaction.
          type: string
          example: '3fa85f64-5717-4562-b3fc-2c963f66afa6'
    authorizationState:
      description: >
        Transaction authorization status


        WAITING_FOR_SIGN


        Waiting for user to authorize transaction


        PROCESSING


        Processing authorization


        DONE


        Transaction has been authorized


        REFUSED


        User or bank has refused the transaction


        CANCELLING


        Authorization is being cancelled


        CANCELED


        Authorization has been cancelled


        EXPIRING


        Authorization is expiring


        EXPIRED


        Authorization has expired

      type: string
      enum: ['WAITING_FOR_SIGN', 'PROCESSING', 'DONE', 'REFUSED', 'CANCELLING', 'CANCELED', 'EXPIRING', 'EXPIRED']
      example: 'WAITING_FOR_SIGN'
    instructionStatusInitial:
      description: >
        Status of entered payment


        ACTC


        [AcceptedTechnicalValidation] - Authentication and syntactical and semantical validation are successful

      type: string
      enum: ['ACTC']
      example: 'ACTC'
    instructionStatus:
      description: >
        Status of entered payment


        ACTC


        [AcceptedTechnicalValidation] - Authentication and syntactical and semantical validation are successful


        RJCT


        [Rejected] - Payment initiation or individual transaction included in the payment initiation has been rejected


        ACSP


        [AcceptedSettlementInProcess] - All preceding checks such as technical validation and customer profile were successful and therefore the payment initiation has been accepted for execution


        ACSC


        [AcceptedSettlementCompleted] - Settlement on the debtor´s account has been completed.Usage: this can be used by the first agent to report to the debtor that the transaction has been completed.Warning: this status is provided for transaction status reasons, not for financial information. It can only be used after bilateral agreement

        CUST


        [RequestedByCustomer] - Cancellation requested by the Debtor


        ACWC


        [AcceptedWithChange] - Instruction is accepted but a change will be made, such as date or remittance not sent

      type: string
      enum: ['ACTC', 'RJCT', 'ACSP', 'ACSC', 'ACWC']
      example: 'ACWC'
    instructionStatusOfStandingOrder:
      description: >
        Status of entered payment


        ACTC


        [AcceptedTechnicalValidation] - Authentication and syntactical and semantical validation are successful


        RJCT


        [Rejected] - Payment initiation or individual transaction included in the payment initiation has been rejected


        ACSP


        [AcceptedSettlementInProcess] - All preceding checks such as technical validation and customer profile were successful and therefore the payment initiation has been accepted for execution


        CUST


        [RequestedByCustomer] - Cancellation requested by the Debtor


        ACSC


        [AcceptedSettlementCompleted] – Standing order on the debtor's account has been completed

      type: string
      enum: ['ACTC', 'RJCT', 'ACSP', 'CUST', 'ACSC']
      example: 'ACSP'
    pollInterval:
      description: Value in milliseconds how often I can make requests.
      type: number
      example: 1000
    statusChangeInfo:
      description: >
        Specifies the change which was made to the instruction. Used with istructionStatus "ACWC" [AcceptedWithChange].


        If there is a change, then the change is specified by relevant error code (see error reponses below).

      type: string
      example: 'DT01'
    requestedExecutionDate:
      description: Requested date of payment - Date in ISO 8601 Date format
      type: string
      format: date
      example: '2017-01-31'
    standingOrder:
      description: Structure describing the parameters of the standing orders.
      type: object
      required:
        - execution
      properties:
        alias:
          description: Description resp. the user-defined standing order payment name.
          type: string
          example: 'Payments for lunch - kindergarten Michal'
        execution:
          description: Features of execution a standing order for payment.
          type: object
          required:
            - mode
            - modeDue
            - interval
            - intervalDue
          properties:
            mode:
              description: >
                The execution mode defines when or how standing order will be cancelled, processed the last time. Possible values:


                UNTIL_CANCELLATION


                standing order is valid forever and must be cancelled by client

              type: string
              enum: ['UNTIL_CANCELLATION']
              example: 'UNTIL_CANCELLATION'
            modeDue:
              description: >
                The execution due mode defines how the date when order should be executed is specified. Possible values:




                DUE_OR_BEFORE_DAY_OF_MONTH


                order is defined as a specific day of the month or the previous day if the day falls on a non-banking day

              type: string
              enum: ['DUE_OR_BEFORE_DAY_OF_MONTH']
              example: 'DUE_OR_BEFORE_DAY_OF_MONTH'
            interval:
              description: >
                Execution interval defines how often order is executed. Possible values: WEEKLY, MONTHLY, QUARTERLY, HALFYEARLY, YEARLY.

              type: string
              enum: ['WEEKLY', 'MONTHLY', 'QUARTERLY', 'HALFYEARLY', 'YEARLY']
              example: 'MONTHLY'
        validity:
          description: Features of execution a standing order for payment.
          type: object
          properties:
            lastExecutionDate:
              description: Date when the last order will be processed. Null value responds to ending by the user.
              allOf:
                - $ref: '#/components/schemas/ISO8601Date'
    standingOrderRealizedPayments:
      description: Array of transaction identificators, conditional if it is an authorized standing order
      type: array
      items:
        type: object
        required:
          - transactionIdentification
        properties:
          transactionIdentification:
            description: Identifier of estabilished transaction
            type: string
            example: 'someId56668746523322'
          date:
            description: Due date/payment foreign currency in the format ISODate, , or ISODateTime, i.e. YYYY-MM-DD, or YYYYMM-DDThh:mm:ss.sTZD, depending on the transaction type and method how the bank presents data (and time) of due date/ payment foreign currency. Mainly for card or cash transactions it is posted as ISODateTime.
            allOf:
              - $ref: '#/components/schemas/ISO8601DateTime'
